<odoo>
    <data>
        <record id="paperformat_report_invoice_report_new" model="report.paperformat">
            <field name="name">Paper Format Invoice Report New</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="format">custom</field>
            <field name="page_height">297</field>
            <field name="page_width">210</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">25</field>
            <field name="margin_bottom">18</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">20</field>
            <field name="dpi">90</field>
        </record>

        <record id="paperformat_report_journal_report_new" model="report.paperformat">
            <field name="name">Paper Format Invoice Report New</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="format">custom</field>
            <field name="page_height">297</field>
            <field name="page_width">210</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">25</field>
            <field name="margin_bottom">6</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">20</field>
            <field name="dpi">90</field>
        </record>
        <report
                id="bran_invoice_pdf_report_id"
                model="account.move"
                report_type="qweb-pdf"
                string="Invoice
                name="invoice_pdf_report.invoice_pdf_report_id"
                file="invoice_pdf_report.invoice_pdf_report_id"
                paperformat="invoice_pdf_report.paperformat_report_invoice_report_new"
        />

        <report
                id="bran_vendor_bill_pdf_report_id"
                model="account.move"
                report_type="qweb-pdf"
                string="Vendor Bill"
                name="invoice_pdf_report.invoice_pdf_report_id"
                file="invoice_pdf_report.invoice_pdf_report_id"
                paperformat="invoice_pdf_report.paperformat_report_invoice_report_new"
                print_report_name="(object.name+'.pdf')"

        />

        <report
                id="bran_journal_entries_pdf_report_id"
                model="account.move"
                report_type="qweb-pdf"
                string="Journal Entries"
                name="invoice_pdf_report.journal_entries_pdf_report_id"
                file="invoice_pdf_report.journal_entries_pdf_report_id"
                paperformat="invoice_pdf_report.paperformat_report_journal_report_new"
                print_report_name="(object.name+'.pdf')"

        />

        <report
                id="bran_sale_pdf_report_id"
                model="sale.order"
                report_type="qweb-pdf"
                string="Techno Commercial Proposal"
                name="invoice_pdf_report.sale_pdf_report_id"
                file="invoice_pdf_report.sale_pdf_report_id"
                paperformat="invoice_pdf_report.paperformat_report_invoice_report_new"
        />

        <report
                id="bran_sale_order_pdf_report_id"
                model="sale.order"
                report_type="qweb-pdf"
                string="Sale Order"
                name="invoice_pdf_report.sale_pdf_report_id"
                file="invoice_pdf_report.sale_pdf_report_id"
                paperformat="invoice_pdf_report.paperformat_report_invoice_report_new"
        />

        <report
                id="purchase_pdf_report_id"
                model="purchase.order"
                report_type="qweb-pdf"
                string="Request For Proposal"
                name="invoice_pdf_report.bran_purchase_pdf_report_id"
                file="invoice_pdf_report.bran_purchase_pdf_report_id"
                paperformat="invoice_pdf_report.paperformat_report_invoice_report_new"
        />


        <report
                id="purchase_order_pdf_report_id"
                model="purchase.order"
                report_type="qweb-pdf"
                string="Purchase Order"
                name="invoice_pdf_report.bran_purchase_pdf_report_id"
                file="invoice_pdf_report.bran_purchase_pdf_report_id"
                paperformat="invoice_pdf_report.paperformat_report_invoice_report_new"
        />
    </data>
</odoo>